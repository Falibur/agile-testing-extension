{
  "name": "ChromePhoneExtension",
	"icons": { "16": "assets/path3038.png",
	           "48": "assets/icon48.png",
	          "128": "assets/icon128.png" },
  "version": "0.5",
  "description": "Send phone numbers and maps to your phone",
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "assets/path3038.png",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file://*/*"],
      "js": ["page.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs", "notifications", "http://*/*",  "https://*/*"
  ]
}